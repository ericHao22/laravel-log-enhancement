{
    "name": "onramplab/laravel-log-enhancement",
    "description": "An enhanced logging package for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Kos Huang",
            "email": "kos.huang@onramplab.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "datadog/dd-trace": "^0.90.0",
        "illuminate/contracts": "*",
        "illuminate/log": "*",
        "illuminate/support": "*",
        "monolog/monolog": "^3.4",
        "ramsey/uuid": "^4.0.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.3.2",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.10",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Onramplab\\LaravelLogEnhancement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onramplab\\LaravelLogEnhancement\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Onramplab\\LaravelLogEnhancement\\LaravelLogEnhancementServiceProvider"
            ]
        }
    }
}
